<ul>
	<% @folders.each do |s| %>
		<% if s && s.length > 0 %>
			<li class="folder-name">
				<h4><%= s %></h4>
			</li>

			<% @photos.each do |p| %>
				<% if p.folder == s %>
					<li class="photo-name">
						<%= link_to p.name, show_photo_project_path(@project,:photo_id => p.id), :remote => true %>
					</li>
				<% end %>
			<% end %>
			
		<% end %>
	<% end %>
</ul>