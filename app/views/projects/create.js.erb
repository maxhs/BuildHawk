$('#projects').prepend('<%= j render partial:"projects/project", locals: {project: @project} %>');
$('#main').html('<%= j render partial:"projects/index", locals: {projects: @projects} %>');
if (history && history.pushState){
    history.pushState(null, null, '<%=root_url%>projects');
    $(window).bind("popstate", function(){
      	$.getScript(location.href);
    });
}
$('#sidebar-progress-link-<%=@project.id%>').progressbar({value:0});