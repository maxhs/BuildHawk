$('#filters > li > a').removeClass('current-page');
$('#<%= @nav %>').addClass('current-page');
$('#photo-focus').fadeOut(170,function(){
	$(this).html('<%= j render :partial => "projects/large_image" %>');
	$(this).removeClass('full-focus');
	$(this).fadeIn(170);
});
$('#photo-table').fadeOut(170,function(){
	$(this).html('<%= j render :partial => "projects/folders" %>');
	$(this).fadeIn(170);
});
Shadowbox.clearCache();
Shadowbox.setup(".shadow-photo", {});