
var newDiv = $('<%= j render partial:"phases/new" %>').css('display','none');
$('#new-phase').before(newDiv);
newDiv.fadeIn(200);
$('#new-phase').hide();
$('#dismiss-new-phase').click(function(){
	$('.new_phase').slideUp(230,function(){
		$('.new_phase').remove();
	});
	$('#new-phase').show(230);
});